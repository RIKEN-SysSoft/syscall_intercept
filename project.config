[access]
	inheritFrom = All-Projects
[submit]
	mergeContent = true
	action = rebase if necessary
[access "refs/*"]
	owner = group riken
[access "refs/for/refs/*"]
	read = group riken
	push = group riken
	pushMerge = group riken
[access "refs/heads/*"]
	read = group Anonymous Users
	create = group riken
	push = +force group riken
	pushMerge = group riken
	label-Code-Review = -2..+2 group riken
	label-Verified = -1..+1 group riken
	label-Verified = -1..+1 group mckernel-bots
	rebase = group riken
	submit = group riken
[access "refs/tags/*"]
	push = group riken
	push = +force group riken
	read = group Anonymous Users
	delete = group riken
[commentlink "redmine"]
	match = "#(\\d+)"
	link = https://postpeta.pccluster.org/redmine/issues/$1
[receive]
	requireChangeId = true
[project]
	description = The system call intercepting library
[label "Verified"]
       defaultValue = +1
